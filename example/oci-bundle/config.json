{
    "version": "0.0.1",
    "platform": {
        "os": "linux",
        "arch": "amd64"
    },
    "process": {
        "terminal": false,
        "user": {
            "uid": 0,
            "gid": 0
        },
        "args": [
            "/usr/bin",
            "bash"
        ],
        "env": [
            "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
            "TERM=xterm"
        ],
        "cwd": "/testtool"
    },
    "root": {
        "path": "./rootfs_rootconfig",
        "readonly": true
    },
    "hostname": "bash",
    "mounts": [
        {
            "name": "proc",
            "path": "/proc"
        },
        {
            "name": "dev",
            "path": "/dev"
        },
        {
            "name": "devpts",
            "path": "/dev/pts"
        },
        {
            "name": "data",
            "path": "/data"
        }
    ]
}
